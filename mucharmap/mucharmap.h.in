#ifndef MUCHARMAP_H
#define MUCHARMAP_H

#define __MUCHARMAP_MUCHARMAP_H_INSIDE__

#define MUCHARMAP_VERSION_MAJOR    (@MUCHARMAP_VERSION_MAJOR@)
#define MUCHARMAP_VERSION_MINOR    (@MUCHARMAP_VERSION_MINOR@)
#define MUCHARMAP_VERSION_MICRO    (@MUCHARMAP_VERSION_MICRO@)

#define MUCHARMAP_CHECK_VERSION(major, minor, micro) \
	(MUCHARMAP_VERSION_MAJOR > (major) || \
	(MUCHARMAP_VERSION_MAJOR == (major) && MUCHARMAP_VERSION_MINOR > (minor)) || \
	(MUCHARMAP_VERSION_MAJOR == (major) && MUCHARMAP_VERSION_MINOR == (minor) && MUCHARMAP_VERSION_MICRO >= (micro)))

#include <mucharmap/mucharmap-codepoint-list.h>
#include <mucharmap/mucharmap-block-codepoint-list.h>
#include <mucharmap/mucharmap-script-codepoint-list.h>

#include <mucharmap/mucharmap-chapters-model.h>
#include <mucharmap/mucharmap-block-chapters-model.h>
#include <mucharmap/mucharmap-script-chapters-model.h>

#include <mucharmap/mucharmap-chartable.h>
#include <mucharmap/mucharmap-charmap.h>

#include <mucharmap/mucharmap-type-builtins.h>

#include <mucharmap/mucharmap-unicode-info.h>

#undef __MUCHARMAP_MUCHARMAP_H_INSIDE__

#endif /* MUCHARMAP_H */
